{
  "storage": {
    "projectlist": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "projectManager": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "projectlist",
          "attributes": ["Name", "Arn"]
        }
      ]
    }
  },
  "api": {
    "projectAPI": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "projectManager",
          "attributes": ["Name", "Arn"]
        }
      ]
    }
  }
}
